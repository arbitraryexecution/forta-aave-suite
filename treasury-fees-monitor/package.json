{
  "name": "forta-aave-treasury-fees-monitor",
  "version": "0.0.1",
  "description": "Bot that monitors FlashLoan events and alerts when the amount of fees collected is greater than the configured threshold",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec 'forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest --detectOpenHandles",
    "test:docker": "docker build -t testbuild .",
    "posttest:docker": "docker run testbuild",
    "collect": "node ./data-collection/collect-fee-data.js"
  },
  "dependencies": {
    "bignumber.js": "^9.0.2",
    "command-line-args": "^5.2.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.5",
    "forta-agent": "^0.1.4",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.0.6",
    "nodemon": "^2.0.8"
  }
}
