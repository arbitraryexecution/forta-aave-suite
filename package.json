{
  "name": "forta-ave-suite",
  "version": "0.0.1",
  "description": "Collection of scan agent handlers that monitor the Aave protocol",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec 'forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "test": "jest"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "forta-agent": "^0.0.11",
    "rolling-math": "git+ssh://git@github.com/arbitraryexecution/rolling-math.git"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.0.6",
    "nodemon": "^2.0.8"
  }
}
